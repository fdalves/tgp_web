<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="2.2" xmlns="http://xmlns.jcp.org/xml/ns/javaee"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
 <converter>
  <converter-id>usuarioConverter</converter-id>
  <converter-class>util.UsuarioConverter</converter-class>
 </converter>
 <navigation-rule>
  <from-view-id>/pages/login.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>go_initTgp</from-outcome>
   <to-view-id>/pages/initTgp.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/pages/initTgp.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>go_user</from-outcome>
   <to-view-id>/pages/crudUsuario.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>go_proj</from-outcome>
   <to-view-id>/pages/crudProjeto.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>go_ativ</from-outcome>
   <to-view-id>/pages/crudAtividade.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <referenced-bean>
  <referenced-bean-name>usuarioMB</referenced-bean-name>
  <referenced-bean-class>managedbean.UsuarioMB</referenced-bean-class>
 </referenced-bean>
 <referenced-bean>
  <referenced-bean-name>projetoMB</referenced-bean-name>
  <referenced-bean-class>managedbean.ProjetoMB</referenced-bean-class>
 </referenced-bean>
 <referenced-bean>
  <referenced-bean-name>atividadeMB</referenced-bean-name>
  <referenced-bean-class>managedbean.AtividadeMB</referenced-bean-class>
 </referenced-bean>
 <referenced-bean>
  <referenced-bean-name>loginMB</referenced-bean-name>
  <referenced-bean-class>managedbean.LoginMB</referenced-bean-class>
 </referenced-bean>
 <referenced-bean>
  <referenced-bean-name>initTgpMB</referenced-bean-name>
  <referenced-bean-class>managedbean.InitTgpMB</referenced-bean-class>
 </referenced-bean>
 <validator>
  <validator-id>custom.emailValidator</validator-id>
  <validator-class>util.EmailValidator</validator-class>
 </validator>
 <application/>
</faces-config>
